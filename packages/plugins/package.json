{
  "name": "@mokr/plugins",
  "version": "1.8.0",
  "description": "Moker plugins",
  "repository": "https://github.com/hongaar/moker",
  "license": "MIT",
  "author": "joram@vandenboezem.nl",
  "type": "module",
  "exports": {
    "import": "./dist/index.js",
    "types": "./types/index.d.ts"
  },
  "files": [
    "dist",
    "types",
    "static"
  ],
  "scripts": {
    "build": "yarn clean && tsc --build --force",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist && rm -rf types",
    "prepublish": "yarn build"
  },
  "dependencies": {
    "@mokr/core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "20.7.0",
    "typescript": "5.2.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "moker": {
    "plugins": [
      "typescript"
    ]
  }
}
